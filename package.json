    "karma-mocha-reporter": "2.0.3",
    "karma-typescript-preprocessor": "0.0.21",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.12",
    "semantic-release": "4.3.5",
    "systemjs": "0.19.27",
